{{log this}}
{{#each this}}
  <div class="group-header">
    <h2>{{name}}</h2>
  </div>
  <div class="container">
    {{#each routines}}
      <div data-item="{{@../index}}-{{@index}}" class="card">
        {{#if week}}
          <h1>Week {{week}} - Session {{session}}</h1>
        {{else}}
          <h1>{{name}}</h1>
        {{/if}}
      </div>
    {{/each}}
  </div>
{{/each}}